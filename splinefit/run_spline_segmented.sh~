#!/bin/sh

seed=1001 #1005
# 2521359361 - 500000
# = 2520859361

# 500000 * 100 = 50000000 --> 100 jobs 

# init, step, end
#for i in `seq 0 500000 2521359361`
for i in `seq 0 500000 2520500000` #1258000000
do echo $i
    bsub -o bsub_spline_runs/${i}.o python3 2_spline_interpolation_segmented.py $i $seed
done




